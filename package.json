{
  "name": "lms",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "catw": "^1.0.1",
    "lessc-each": "^1.1.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0"
  },
  "scripts": {
    "build-css": "lessc-each src/less/ src/css/",
    "watch-less": "catw -wc 'npm run build-css' src/less/*",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-less start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
