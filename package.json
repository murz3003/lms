{
  "name": "lms",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-preset-es2015": "^6.24.0",
    "catw": "^1.0.1",
    "lessc-each": "^1.1.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.1",
    "react-scripts": "0.9.0"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "moment": "^2.17.1",
    "moment-timezone": "^0.5.11",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-moment": "^0.2.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "restify": "^4.3.0"
  },
  "scripts": {
    "build-css": "lessc-each src/less/ src/css/",
    "watch-less": "catw -wc 'npm run build-css' src/less/*.less",
    "start-js": "react-scripts start",
    "server-dev": "nodemon --inspect --watch api --exec babel-node api/index.js --presets es2015",
    "start": "npm-run-all -p watch-less start-js server-dev",
    "server-build": "babel api -d build/api --presets es2015",
    "build": "react-scripts build && npm-run-all -p build-css server-build",
    "prod": "node build/api/index.js",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
